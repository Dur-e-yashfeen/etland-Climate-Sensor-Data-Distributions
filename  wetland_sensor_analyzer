   ],
   "source": [
    "# Compute summary statistics\n",
    "summary = df['temperature_C'].describe().to_frame().T\n",
    "summary['IQR'] = summary['75%'] - summary['25%']\n",
    "\n",
    "# Detect outliers\n",
    "q1, q3 = np.percentile(df['temperature_C'], [25, 75])\n",
    "iqr = q3 - q1\n",
    "lower, upper = q1 - 1.5 * iqr, q3 + 1.5 * iqr\n",
    "outliers = df[(df['temperature_C'] < lower) | (df['temperature_C'] > upper)]\n",
    "\n",
    "summary['Outliers'] = len(outliers)\n",
    "\n",
    "print(\"ðŸ“Š Summary Statistics for Global Land Average Temperature\")\n",
    "display(summary)"
   ]
  },
